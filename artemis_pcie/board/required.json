{
  "BASE_DIR":"~/projects/ibuilder_project",
  "board":"artemis_pcie",
  "PROJECT_NAME":"artemis_pcie_default",
  "TEMPLATE":"wishbone_template.json",
  "INTERFACE":{
    "filename":"artemis_pcie_interface.v",
    "bind":{
    }
  },
  "SLAVES":{
    "gpio1":{
      "filename":"wb_gpio.v",
      "bind":{
        "gpio_out[1:0]":{
          "loc":"led[1:0]",
          "direction":"output"
        },
        "gpio_in[3:2]":{
          "loc":"button[1:0]",
          "direction":"input"
        }
      }
    }
  },
  "board_bind":{
    "rst":{
      "direction":"input",
      "loc":"adapter_rst"
    },
    "clk":{
      "direction":"input",
      "loc":"clk"
    }
  }
}
